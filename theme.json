{
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"layout": {
			"contentSize": "800px",
			"wideSize": "1100px",
			"allowEditing": true
		},
		"color": {
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#FFFFFF",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#111111",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#7f39fa",
					"name": "Accent 1",
					"slug": "accent-1"
				},
				{
					"color": "#eee5ff",
					"name": "Accent 2",
					"slug": "accent-2"
				},
				{
					"color": "#f1f1f1",
					"name": "Accent 3",
					"slug": "accent-3"
				},
				{
					"color": "#686868",
					"name": "Accent 4",
					"slug": "accent-4"
				},
				{
					"color": "#FBFAF3",
					"name": "Accent 5",
					"slug": "accent-5"
				},
				{
					"color": "color-mix(in srgb, currentColor 20%, transparent)",
					"name": "Accent 6",
					"slug": "accent-6"
				}
			]
		},
		"typography": {
			"customFontSize": true,
			"defaultFontSizes": false,
			"fluid": true,
			"fontSizes": [
				{
					"fluid": false,
					"name": "Small",
					"size": "0.875rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.375rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.38rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.5rem"
					},
					"name": "Extra Large",
					"size": "1.75rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "3rem",
						"min": "2.15rem"
					},
					"name": "Extra Extra Large",
					"size": "2.15rem",
					"slug": "xx-large"
				}
			],
			"fontFamilies": [
				{
					"fontFamily": "ui-sans-serif, -apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, \"Helvetica Neue\", sans-serif",
					"name": "System Sans Serif",
					"slug": "system-sans-serif"
				},
				{
					"fontFamily": "ui-serif, Georgia, serif",
					"name": "System Serif",
					"slug": "system-serif"
				},
				{
					"fontFamily": "ui-monospace, Menlo, Consolas, Monaco, \"Liberation Mono\", \"Lucida Console\", monospace",
					"name": "System Monospace",
					"slug": "system-monospace"
				}
			]
		}
	},
	"styles": {
		"css": ".round-image-container img {\nborder-radius: 50%;\n}\n\n.wp-block-media-text>.wp-block-media-text__content {\nword-break: default;\n}h1.wp-block-query-title,.wp-block-term-description {max-width: var(--wp--style--global--wide-size);}",
		"typography": {
			"fontFamily": "var:preset|font-family|system-sans-serif",
			"fontSize": "var:preset|font-size|large",
			"fontWeight": "400",
			"letterSpacing": "",
			"lineHeight": "1.5"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var:preset|color|accent-1",
					"text": "var:preset|color|base"
				},
				":focus": {
					"outline": {
						"color": "var:preset|color|accent-4",
						"offset": "2px"
					}
				},
				":hover": {
					"color": {
						"background": "color-mix(in srgb, var(--wp--preset--color--accent-1) 85%, transparent)"
					},
					"border": {
						"color": "transparent"
					}
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent-1)"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "clamp(1.5rem, 3vw, 2rem)",
					"fontWeight": "900"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(1.5rem, 3vw, 2rem)",
					"fontWeight": "900"
				}
			}
		},
		"blocks": {
			"core/site-title": {
				"css": "text-shadow: clamp(1px, .15vw, 2px) clamp(1px, .15vw, 2px) 0px color-mix(in srgb, var(--wp--preset--color--accent-1) 85%, transparent)",
				"elements": {
					"link": {
						"spacing": {
							"padding": {
								"right": ".5rem",
								"left": ".5rem"
							}
						},
						":hover": {
							"color": {
								"background": "var:preset|color|accent-1",
								"text": "var:preset|color|base"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						"typography": {
							"textDecoration": "none",
							"fontWeight": "900"
						}
					}
				},
				"typography": {
					"fontSize": "clamp(2rem, 5vw, 3.75rem)",
					"fontFamily": "inherit",
					"fontStyle": "inherit",
					"fontWeight": "900",
					"textTransform": "inherit"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "clamp(1.75rem, 3vw, 3rem)",
					"fontWeight": "900"
				},
				"spacing": {
					"margin": {
						"bottom": "var:preset|spacing|20"
					}
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--accent-1)"
							}
						}
					}
				}
			},
			"core/post-author-name": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"fontWeight": "600"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"fontWeight": "600"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"textDecoration": "none"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-comments-form": {
				"color": {
					"background": "var:preset|color|accent-3"
				},
				"spacing": {
					"padding": {
						"top": "var:preset|spacing|20",
						"bottom": "var:preset|spacing|20",
						"right": "var:preset|spacing|30",
						"left": "var:preset|spacing|30"
					},
					"margin": {
						"bottom": "var:preset|spacing|60"
					}
				}
			},
			"core/comment-date": {
				"color": {
					"text": "var:preset|color|accent-4"
				},
				"elements": {
					"link": {
						"css": "text-decoration: none",
						"color": {
							"text": "var:preset|color|accent-1"
						},
						":hover": {
							"color": {
								"text": "color-mix(in srgb, var(--wp--preset--color--accent-1) 85%, transparent)"
							}
						}
					}
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var:preset|font-size|large",
					"fontWeight": "900"
				},
				"color": {
					"text": "var:preset|color|contrast"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|accent-1"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|accent-1"
							}
						}
					}
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				},
				"color": {
					"text": "var:preset|color|contrast",
					"background": "var:preset|color|accent-3"
				},
				"spacing": {
					"padding": {
						"top": ".25rem",
						"bottom": ".25rem",
						"right": ".75rem",
						"left": ".75rem"
					},
					"margin": {
						"bottom": "var:preset|spacing|30"
					}
				},
				"border": {
					"radius": "100px"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|accent-4"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|accent-1"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var:preset|font-size|small"
				},
				"color": {
					"text": "var:preset|color|contrast",
					"background": "var:preset|color|accent-3"
				},
				"spacing": {
					"padding": {
						"top": ".25rem",
						"bottom": ".25rem",
						"right": ".75rem",
						"left": ".75rem"
					}
				},
				"border": {
					"radius": "100px"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var:preset|color|accent-4"
						},
						":hover": {
							"color": {
								"text": "var:preset|color|accent-1"
							}
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontWeight": "600"
				}
			},
			"core/categories": {
				"css": "list-style: none;padding:0;",
				"typography": {
					"fontWeight": "300"
				},
				"color": {
					"text": "var:preset|color|accent-4"
				},
				"elements": {
					"link": {
						"typography": {
							"fontWeight": "500",
							"textDecoration": "none"
						}
					}
				}
			},
			"core/term-description": {
				"typography": {
					"fontSize": "var:preset|font-size|large"
				},
				"color": {
					"text": "var:preset|color|accent-4"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontWeight": "600",
					"fontSize": "var:preset|font-size|large"
				},
				"color": {
					"text": "var:preset|color|accent-4"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
		}
	}
}